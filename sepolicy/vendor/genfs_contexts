# Touchpanel
genfscon sysfs /touchpanel                                        u:object_r:sysfs_touchpanel:s0

# Also add FTS gesture mode into touchpanel contexts
genfscon sysfs /devices/platform/11007000.i2c0/i2c-0/0-0038/fts_gesture_mode u:object_r:sysfs_touchpanel:s0
# Vibrator nodes
genfscon sysfs /devices/platform/aw8622 u:object_r:sysfs_vibrator:s0
genfscon sysfs /devices/platform/1000d000.pwrap/1000d000.pwrap:mt6359-pmic/mt6359-rtc/rtc/rtc0/hctosys u:object_r:sysfs_rtc:s0
